 <script type="text/javascript">

  var tweetMarkers = <%= raw @markers.to_json %>

  handler = Gmaps.build('Google');
  handler.buildMap({ provider: {styles: getMyMapStyleOne()}, internal: {id: 'tweets-visualizer-map'}}, function(){
    markers = handler.addMarkers(tweetMarkers);
    handler.bounds.extendWith(markers);
    handler.fitMapToBounds();
  });

</script>